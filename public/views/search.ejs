<% include partials/header %>
<div class = "opaque-bg">
<div class = "author-container">
<% posts.forEach(function(post){ %>
    <p class = "version-choice"><a href="/read/<%= post._id %>"><%= post.summary %> written by <%= post.author %> with <%= post.popularity %> upvotes </a></p>
<% }); %>
</div>
<p class = "back-button"><a class = "back-link" href="#" onclick="history.go(-1)">Go back</a></p>
<% include partials/footer %>